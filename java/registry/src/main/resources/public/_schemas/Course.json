          "$id": "#properties/name",
          "type": "string",
          "title": "CourseName"
        },
        "scheme": {
          "$id": "#properties/scheme",
          "type": "string",
          "enum": [
            "PMKVY", "DDUGKY", "ITI"
          ],
          "title": "Scheme"
        },
        "duration": {
          "$id": "#properties/duration",
          "type": "string",
          "title": "Duration"
        }
      }
    }
  }
